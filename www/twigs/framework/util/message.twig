    {%if fwerrmessage is defined %}
        {% for msg in fwerrmessage %}
            <div class="alert alert-danger">{{eraw is defined ? msg|raw : msg}}</div>
        {% endfor %}
    {% endif %}
    {%if fwwarnmessage is defined %}
        {% for msg in fwwarnmessage %}
            <div class="alert alert-warning">{{wraw is defined ? msg|raw : msg}}</div>
        {% endfor %}
    {% endif %}
    {%if fwmessage is defined %}
        {% for msg in fwmessage %}
            <div class="alert alert-info">{{mraw is defined ? msg|raw : msg}}</div>
        {% endfor %}
    {% endif %}
